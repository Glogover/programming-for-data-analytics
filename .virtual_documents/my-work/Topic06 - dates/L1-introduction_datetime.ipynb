print (f"microseconds: {datetime.timedelta(microseconds=1)}")
print (f"milliseconds: {datetime.timedelta(milliseconds=1)}")
print (f"seconds:      {datetime.timedelta(seconds=1)}")
print (f"minutes:      {datetime.timedelta(minutes=1)}")
