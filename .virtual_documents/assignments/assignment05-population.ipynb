


# Imports

import pandas as pd
import numpy as np





url = "https://ws.cso.ie/public/api.restful/PxStat.Data.Cube_API.ReadDataset/FY006A/CSV/1.0/en"
df = pd.read_csv(url)
df.head(5)


df = df[df["Sex"] != "Both sexes"]
df.head(5)


headers = df.columns.tolist()
headers


drop_col_list = ['STATISTIC', 'Statistic Label','TLIST(A1)','C02199V02655','C02076V03371','C03789V04537','UNIT']
df.drop(columns=drop_col_list, inplace=True)


df = df[df["Single Year of Age"] != "All ages"].copy()

# ensure string before using .str
df['Single Year of Age'] = df['Single Year of Age'].astype(str)

# replace label
df['Single Year of Age'] = df['Single Year of Age'].str.replace('Under 1 year', '0')

# remove non-digits
df['Single Year of Age'] = df['Single Year of Age'].str.replace(r'\D', '', regex=True)

# convert safely to numeric
df['Single Year of Age'] = pd.to_numeric(df['Single Year of Age'], errors='coerce')
df['VALUE'] = pd.to_numeric(df['VALUE'], errors='coerce')

df = df.dropna(subset=['Single Year of Age', 'VALUE']).copy()

df['Single Year of Age'] = df['Single Year of Age'].astype('int64')
df['VALUE'] = df['VALUE'].astype('int64')

print(df.head())



df_analysis = pd.pivot_table(df, 'VALUE',"Single Year of Age","Sex")
print (df_analysis.head(71))


df_analysis = pd.pivot_table(df, 'VALUE',"Single Year of Age","Sex")
print (df_analysis.tail(30))


# write out the entire file
df_analysis.to_csv("population_by_age_for_analysis.csv")


headers = list(df_analysis.columns)
female = headers[0]
#female


headers = list(df_analysis.columns)
male = headers[1]
#male


# Weighted mean age (women)

w_mean_women = np.average(df_analysis.index, weights=df_analysis[female])
w_mean_women


# Weighted mean age (men)

w_mean_men = np.average(df_analysis.index, weights=df_analysis[male])
w_mean_men





age = 35


df = df.copy()
df['age_numeric'] = pd.to_numeric(df['Single Year of Age'], errors='coerce')


lower_age = age - 5
upper_age = age + 5


age_mask = df['age_numeric'].ge(lower_age) & df['age_numeric'].le(upper_age)
group_df = df[age_mask].copy()
#group_df


# Aggregate population by sex

population_by_sex = group_df.groupby('Sex', dropna=False)['VALUE'].sum()
population_by_sex;


# Calculate difference (Male - Female)

male_population = population_by_sex.get('Male', 0)
female_population = population_by_sex.get('Female', 0)
difference = female_population - male_population
difference


